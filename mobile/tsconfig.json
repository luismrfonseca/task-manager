{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "../shared/*"
      ],
      "class-validator": [
        "./node_modules/class-validator"
      ],
      "class-transformer": [
        "./node_modules/class-transformer"
      ]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}